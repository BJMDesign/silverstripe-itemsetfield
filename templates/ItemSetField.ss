	<ul>
	<% control ItemForms %>$ForTemplate<% end_control %>
	</ul>

	<div class='item-set-field-actions'>	
		<% control Actions %>
			<input class='item-set-field-action $ExtraClass' type='button' value='$Name' rel='$Link'/>
		<% end_control %>
	</div>
</div>


